    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.1",
    "@radix-ui/react-tooltip": "^1.1.2",
    "cheerio": "^1.1.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^16.6.0",
    "firebase": "^11.9.1",
    "firebase-admin": "^12.7.0",
    "firebase-functions": "^6.3.2",
    "framer-motion": "^12.18.1",
    "genkit": "^1.13.0",
