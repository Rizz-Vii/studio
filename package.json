  "private": true,
  "description": "RankPilot - AI-First SEO Platform (Internal: Studio)",
  "scripts": {
    "dev": "cross-env NODE_OPTIONS='--max-old-space-size=3072' next dev",
    "dev:instant": "cross-env NODE_OPTIONS='--max-old-space-size=3072' LEAN_MODE=true next dev",
    "genkit:dev": "cross-env NODE_OPTIONS='--max-old-space-size=2048' genkit start -- tsx src/ai/dev.ts",
    "build": "cross-env NODE_OPTIONS='--max-old-space-size=6144' next build",
    "build:lean": "cross-env NODE_OPTIONS='--max-old-space-size=4096' LEAN_MODE=true SKIP_TYPE_CHECK=true next build",
    "build:production": "npm run clean && npm run optimize && npm run build",
    "start": "cross-env NODE_OPTIONS='--max-old-space-size=2048' next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit --incremental",
    "type-check": "tsc --noEmit --incremental",
    "lint:check": "next lint",
    "analyze": "cross-env ANALYZE=true npm run build",
    "clean": "rimraf .next tsconfig.tsbuildinfo",
    "optimize": "ts-node scripts/optimize-project.ts",
    "security-check": "npm audit && npm run verify-env",
    "verify-env": "echo 'âœ… Environment verification completed'",
    "test:mobile": "cross-env NODE_OPTIONS='--max-old-space-size=2048' playwright test --grep mobile",
    "test:accessibility": "cross-env NODE_OPTIONS='--max-old-space-size=2048' playwright test --grep accessibility",
    "test:performance": "cross-env NODE_OPTIONS='--max-old-space-size=2048' playwright test --grep performance",
    "test:critical": "cross-env NODE_OPTIONS='--max-old-space-size=2048' playwright test --grep critical",
    "test:preview": "cross-env NODE_OPTIONS='--max-old-space-size=2048' TEST_BASE_URL=https://rankpilot-h3jpc--performance-testing-mw0cwov5.web.app playwright test",
    "precommit": "npm run lint:fix && npm run typecheck && npm run optimize",
    "test": "cross-env NODE_OPTIONS='--max-old-space-size=2048' playwright test",
    "test:production": "cross-env NODE_OPTIONS='--max-old-space-size=2048' TEST_BASE_URL=https://rankpilot-h3jpc.web.app playwright test",
    "deploy:instant": "./scripts/deploy-lean-instant.ps1",
    "deploy:instant-skip-build": "./scripts/deploy-lean-instant.ps1 -SkipBuild",
    "hyperloop": "git push && npm run deploy:instant",
    "hyperloop:dryrun": "./scripts/deploy-lean-instant.ps1 -DryRun",
    "hyperloop:dev": "node -e \"process.platform === 'win32' ? require('child_process').spawn('powershell', ['-ExecutionPolicy', 'Bypass', '-File', '.\\\\pilotScripts\\\\workflow\\\\hyperloop-dev.ps1'], {stdio: 'inherit'}) : require('child_process').spawn('bash', ['./pilotScripts/workflow/hyperloop-linux.sh'], {stdio: 'inherit'})\"",
    "hyperloop:dev:bash": "bash ./pilotScripts/workflow/hyperloop-dev.sh",
    "hyperloop:linux": "bash ./pilotScripts/workflow/hyperloop-linux.sh",
    "hyperloop:docs": "code ./docs/DEVELOPMENT_HYPERLOOP.md",
    "test:lean": "cross-env NODE_OPTIONS='--max-old-space-size=2048' playwright test -c playwright.config.lean.ts",
    "test:lean:url": "cross-env NODE_OPTIONS='--max-old-space-size=2048' TEST_URL=$(firebase hosting:channel:list --project rankpilot-h3jpc --json | grep -o '\"url\":\"[^\"]*' | head -1 | cut -d'\"' -f4) playwright test -c playwright.config.lean.ts"
  },
