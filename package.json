{
  "name": "nextn",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_OPTIONS='--max-old-space-size=2048' next dev",
    "dev-turbopack": "cross-env NODE_OPTIONS='--max-old-space-size=2048' next dev --turbopack",
    "genkit:dev": "cross-env NODE_OPTIONS='--max-old-space-size=1024' genkit start -- tsx src/ai/dev.ts",
    "genkit:watch": "cross-env NODE_OPTIONS='--max-old-space-size=1024' genkit start -- tsx --watch src/ai/dev.ts",
    "build": "cross-env NODE_OPTIONS='--max-old-space-size=4096' next build --no-lint",
    "start": "cross-env NODE_OPTIONS='--max-old-space-size=1024' next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "rotate-credentials": "ts-node scripts/rotate-credentials.ts",
    "verify-env": "ts-node -P scripts/tsconfig.json scripts/verify-env.ts",
    "security-check": "npm audit && npm run verify-env",
    "test": "cross-env NODE_OPTIONS='--max-old-space-size=1024' playwright test",
    "test:local": "cross-env NODE_OPTIONS='--max-old-space-size=1024' playwright test --config=playwright.config.local.ts",
    "test:local:basic": "cross-env NODE_OPTIONS='--max-old-space-size=1024' playwright test --config=playwright.config.simple.ts",
    "test:local:simple": "cross-env NODE_OPTIONS='--max-old-space-size=1024' playwright test --config=playwright.config.simple.ts tests/dev-ready.spec.ts",
    "test:local:debug": "cross-env NODE_OPTIONS='--max-old-space-size=1024' playwright test --config=playwright.config.simple.ts --headed --debug",
    "test:prod": "cross-env NODE_OPTIONS='--max-old-space-size=1024' TEST_BASE_URL=https://rankpilot-h3jpc.web.app playwright test",
    "test:headed": "cross-env NODE_OPTIONS='--max-old-space-size=1024' playwright test --headed",
    "test:headed:local": "cross-env NODE_OPTIONS='--max-old-space-size=1024' playwright test --config=playwright.config.local.ts --headed",
    "test:headed:prod": "cross-env NODE_OPTIONS='--max-old-space-size=1024' TEST_BASE_URL=https://rankpilot-h3jpc.web.app playwright test --headed",
    "test:ui": "cross-env NODE_OPTIONS='--max-old-space-size=1024' playwright test --ui",
    "test:visual": "cross-env NODE_OPTIONS='--max-old-space-size=1024' playwright test --project=visual",
    "test:update-snapshots": "cross-env NODE_OPTIONS='--max-old-space-size=1024' playwright test --update-snapshots",
    "analyze": "cross-env ANALYZE=true npm run build"
  },
  "dependencies": {
    "@genkit-ai/firebase": "^1.14.0",
    "@genkit-ai/googleai": "^1.13.0",
    "@google-cloud/error-reporting": "^3.0.5",
    "@hookform/resolvers": "^5.1.1",
    "@langchain/core": "^0.3.62",
    "@opentelemetry/auto-instrumentations-node": "^0.52.0",
    "@opentelemetry/resources": "^2.0.1",
    "@opentelemetry/sdk-node": "^0.203.0",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-avatar": "^1.1.0",
    "@radix-ui/react-checkbox": "^1.1.1",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-menubar": "^1.1.15",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.0",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
