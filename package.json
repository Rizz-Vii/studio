{
  "name": "rankpilot-studio",
  "version": "1.0.0",
  "private": true,
  "description": "RankPilot - AI-First SEO Platform (Internal: Studio)",
  "scripts": {
    "dev": "cross-env NODE_OPTIONS='--max-old-space-size=3072' next dev --turbopack",
    "dev:webpack": "cross-env NODE_OPTIONS='--max-old-space-size=3072' next dev",
    "dev-no-turbopack": "cross-env NODE_OPTIONS='--max-old-space-size=3072' next dev",
    "dev:turbo": "cross-env NODE_OPTIONS='--max-old-space-size=3072' next dev --turbopack",
    "dev:fast": "cross-env NODE_OPTIONS='--max-old-space-size=3072' next dev --turbopack --experimental-https",
    "dev:clean": "npm run clean && cross-env NODE_OPTIONS='--max-old-space-size=3072' next dev",
    "genkit:dev": "cross-env NODE_OPTIONS='--max-old-space-size=2048' genkit start -- tsx src/ai/dev.ts",
    "genkit:watch": "cross-env NODE_OPTIONS='--max-old-space-size=2048' genkit start -- tsx --watch src/ai/dev.ts",
    "build": "cross-env NODE_OPTIONS='--max-old-space-size=8192' next build",
    "build:analyze": "cross-env ANALYZE=true NODE_OPTIONS='--max-old-space-size=8192' next build",
    "build:fast": "cross-env NODE_OPTIONS='--max-old-space-size=6144' next build",
    "build:emergency": "node scripts/build-skip-typecheck.js",
    "build:production": "npm run clean && npm run optimize && npm run build",
    "start": "cross-env NODE_OPTIONS='--max-old-space-size=2048' next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "format:docs": "prettier --write \"docs/**/*.md\"",
    "format:docs:check": "prettier --check \"docs/**/*.md\"",
    "lint:md": "markdownlint \"docs/**/*.md\"",
    "lint:md:fix": "markdownlint \"docs/**/*.md\" --fix",
    "typecheck": "tsc --noEmit --incremental",
    "rotate-credentials": "ts-node scripts/rotate-credentials.ts",
    "migrate-tiers": "ts-node -P scripts/tsconfig.json scripts/migrate-tier-consistency.ts",
    "update-tier-system": "ts-node -P scripts/tsconfig.json scripts/update-tier-system.ts",
    "fix-subscription-tiers": "ts-node -P scripts/tsconfig.json scripts/fix-subscription-tiers.ts",
    "verify-env": "ts-node -P scripts/tsconfig.json scripts/verify-env.ts",
    "security-check": "npm audit && npm run verify-env",
    "clean": "rimraf .next tsconfig.tsbuildinfo functions/lib testing/results testing/reports",
    "optimize": "ts-node scripts/optimize-project.ts",
    "optimize-windows": "powershell -ExecutionPolicy Bypass -File scripts/optimize-windows-filesystem.ps1",
    "optimize-windows:benchmark": "powershell -ExecutionPolicy Bypass -File scripts/optimize-windows-filesystem.ps1 -Benchmark",
    "optimize-windows:dry-run": "powershell -ExecutionPolicy Bypass -File scripts/optimize-windows-filesystem.ps1 -DryRun",
    "emfile:check": "powershell -ExecutionPolicy Bypass -File scripts/fix-emfile-error.ps1 -Mode check",
    "emfile:fix": "powershell -ExecutionPolicy Bypass -File scripts/fix-emfile-error.ps1 -Mode fix -AutoFix",
    "emfile:monitor": "powershell -ExecutionPolicy Bypass -File scripts/fix-emfile-error.ps1 -Mode monitor",
    "emfile:monitor:verbose": "powershell -ExecutionPolicy Bypass -File scripts/fix-emfile-error.ps1 -Mode monitor -Verbose",
    "emfile:monitor:autofix": "powershell -ExecutionPolicy Bypass -File scripts/fix-emfile-error.ps1 -Mode monitor -AutoFix",
    "emfile:preventive": "powershell -ExecutionPolicy Bypass -File scripts/fix-emfile-error.ps1 -Mode preventive",
    "db:create-auth": "npx ts-node scripts/create-auth-users.ts",
    "db:create-test-users": "node scripts/create-test-users.js",
    "db:populate": "npx ts-node scripts/populate-dummy-data.ts",
    "db:verify": "npx ts-node scripts/verify-dummy-data.ts",
    "db:test-all": "npx ts-node scripts/test-all-use-cases.ts",
    "db:get-users": "npx ts-node scripts/get-all-users.ts",
    "db:setup": "npm run db:populate && npm run db:verify && npm run db:test-all",
    "precommit": "npm run lint:fix && npm run typecheck && npm run optimize",
    "test": "cross-env NODE_OPTIONS='--max-old-space-size=2048' playwright test --config=testing/configs/playwright.config.optimized.ts",
    "test:unit": "cross-env NODE_OPTIONS='--max-old-space-size=2048' playwright test --config=testing/configs/playwright.config.optimized.ts --project=unit",
    "test:integration": "cross-env NODE_OPTIONS='--max-old-space-size=2048' playwright test --config=testing/configs/playwright.config.optimized.ts --project=integration",
    "test:e2e": "cross-env NODE_OPTIONS='--max-old-space-size=2048' playwright test --config=testing/configs/playwright.config.optimized.ts --project=e2e",
    "test:mobile": "cross-env NODE_OPTIONS='--max-old-space-size=2048' playwright test --config=testing/configs/playwright.config.optimized.ts --project=mobile",
    "test:visual": "cross-env NODE_OPTIONS='--max-old-space-size=2048' playwright test --config=testing/configs/playwright.config.optimized.ts --project=visual",
    "test:performance": "cross-env NODE_OPTIONS='--max-old-space-size=2048' playwright test --config=testing/configs/playwright.config.optimized.ts --project=performance",
    "test:accessibility": "cross-env NODE_OPTIONS='--max-old-space-size=2048' playwright test --config=testing/configs/playwright.config.optimized.ts --project=accessibility",
    "test:fast": "cross-env NODE_OPTIONS='--max-old-space-size=2048' playwright test --config=testing/configs/playwright.config.optimized.ts --project=unit --project=integration",
    "test:critical": "cross-env NODE_OPTIONS='--max-old-space-size=2048' playwright test --config=testing/configs/playwright.config.optimized.ts testing/specs/main/auth-consolidated.spec.ts testing/specs/main/basic.spec.ts testing/specs/main/dev-ready.spec.ts",
    "test:local": "cross-env NODE_OPTIONS='--max-old-space-size=2048' playwright test --config=testing/configs/playwright.config.local.ts",
    "test:local:basic": "cross-env NODE_OPTIONS='--max-old-space-size=2048' playwright test --config=testing/configs/playwright.config.simple.ts",
    "test:local:simple": "cross-env NODE_OPTIONS='--max-old-space-size=2048' playwright test --config=testing/configs/playwright.config.simple.ts testing/specs/main/dev-ready.spec.ts",
    "test:local:debug": "cross-env NODE_OPTIONS='--max-old-space-size=2048' playwright test --config=testing/configs/playwright.config.simple.ts --headed --debug",
    "test:prod": "cross-env NODE_OPTIONS='--max-old-space-size=2048' TEST_BASE_URL=https://rankpilot-h3jpc.web.app playwright test --config=testing/configs/playwright.config.optimized.ts",
    "test:headed": "cross-env NODE_OPTIONS='--max-old-space-size=2048' playwright test --config=testing/configs/playwright.config.optimized.ts --headed",
    "test:headed:local": "cross-env NODE_OPTIONS='--max-old-space-size=2048' playwright test --config=testing/configs/playwright.config.local.ts --headed",
    "test:headed:prod": "cross-env NODE_OPTIONS='--max-old-space-size=2048' TEST_BASE_URL=https://rankpilot-h3jpc.web.app playwright test --config=testing/configs/playwright.config.optimized.ts --headed",
    "test:ui": "cross-env NODE_OPTIONS='--max-old-space-size=2048' playwright test --config=testing/configs/playwright.config.optimized.ts --ui",
    "test:update-snapshots": "cross-env NODE_OPTIONS='--max-old-space-size=2048' playwright test --config=testing/configs/playwright.config.optimized.ts --update-snapshots",
    "test:role-based": "node scripts/run-role-based-tests.js",
    "test:role-based:optimized": "cross-env NODE_OPTIONS='--max-old-space-size=2048' playwright test --config=playwright.config.roles.ts --project=role-based-optimized",
    "test:role-based:comprehensive": "cross-env NODE_OPTIONS='--max-old-space-size=2048' playwright test --config=playwright.config.roles.ts --project=role-based-comprehensive",
    "test:role-based:all": "cross-env NODE_OPTIONS='--max-old-space-size=2048' playwright test --config=playwright.config.roles.ts",
    "test:role-based:cross-browser": "cross-env NODE_OPTIONS='--max-old-space-size=2048' playwright test --config=playwright.config.roles.ts --project=role-firefox --project=role-webkit",
    "test:role-based:mobile": "cross-env NODE_OPTIONS='--max-old-space-size=2048' playwright test --config=playwright.config.roles.ts --project=role-mobile-chrome --project=role-mobile-safari",
    "test:role-based:free": "cross-env NODE_OPTIONS='--max-old-space-size=2048' playwright test --config=playwright.config.role-based.ts --project=free-tier-worker",
    "test:role-based:enterprise": "cross-env NODE_OPTIONS='--max-old-space-size=2048' playwright test --config=playwright.config.role-based.ts --project=enterprise-tier-worker",
    "test:role-based:compatibility": "cross-env NODE_OPTIONS='--max-old-space-size=2048' playwright test --config=playwright.config.role-based.ts --project=firefox-compatibility",
    "test:role-based:windows": "powershell -ExecutionPolicy Bypass -File scripts/run-role-based-tests.ps1",
    "analyze": "cross-env ANALYZE=true npm run build"
  },
  "dependencies": {
