{
  "extends": ["next/core-web-vitals", "next/typescript"],
  "rules": {
    "import/no-anonymous-default-export": [
      "error",
      {
        "allowArray": true,
        "allowArrowFunction": true,
        "allowAnonymousClass": true,
        "allowAnonymousFunction": true,
        "allowLiteral": true,
        "allowObject": true,
        "allowNew": true
      }
    ]
  }
}
