{
    "mcpServers": {
        "notes": {
            "command": "npx",
            "args": [
                "-y",
                "@modelcontextprotocol/server-memory"
            ],
            "env": {
                "MCP_SERVER_MAX_MEMORY": "256MB"
            }
        },
        "filesystem": {
            "command": "npx",
            "args": [
                "-y",
                "@modelcontextprotocol/server-filesystem",
                "/workspaces/studio"
            ],
            "env": {
                "MCP_SERVER_MAX_MEMORY": "256MB"
            }
        },
        "github": {
            "command": "npx",
            "args": [
                "-y",
                "@modelcontextprotocol/server-github"
            ],
            "env": {
                "GITHUB_PERSONAL_ACCESS_TOKEN": "",
                "MCP_SERVER_MAX_MEMORY": "256MB"
            },
            "disabled": true
        },
